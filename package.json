{
  "name": "vscode-test-utils",
  "version": "0.0.6",
  "description": "Test utilities for Visual Studio Code.",
  "main": "./out/src/index.js",
  "typings": "./out/src/index.d.ts",
  "publisher": "jedmao",
  "engines": {
    "vscode": "^0.10.1"
  },
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "test": "npm run lint && npm run compile && npm run vscode-test",
    "lint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "compile": "node ./node_modules/vscode/bin/compile -p ./",
    "watch": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "vscode-test": "node ./node_modules/vscode/bin/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jedmao/vscode-test-utils.git"
  },
  "keywords": [
    "vscode",
    "test",
    "utilities"
  ],
  "author": "Jed Mao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jedmao/vscode-test-utils/issues"
  },
  "homepage": "https://github.com/jedmao/vscode-test-utils#readme",
  "devDependencies": {
    "tslint": "^3.4.0",
    "typescript": "^1.8.2",
    "vscode": "^0.10.1"
  }
}
